Bush(Existing){
  *(Existing)
  | !GotPower ? 5% Egg(H,A) / 50% Egg(H,P,1) / 10% Egg(H,P,2) / 5% Egg(H,P,3) : (Growing)
  
  * (Growing)
  | True ? Wait(10000) : (Existing)
 } 
 
